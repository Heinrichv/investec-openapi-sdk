<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/Heinrichv/investec-openapi-sdk</RepositoryUrl>
    <PackageIcon></PackageIcon>
    <PackageTags>Investec OpenApi SDK</PackageTags>
    <Company>Swifthouse Web (Pty) Ltd</Company>
    <Authors>Heinrich Venter</Authors>
    <PackageProjectUrl>https://github.com/Heinrichv/investec-openapi-sdk</PackageProjectUrl>
    <PackageLicenseExpression>(MIT)</PackageLicenseExpression>
    <Description>.NET Core integration library used to easily integrate with the Investec OpenApi</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryType>.NET Standard Class Library</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="2.2.0" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
    <PackageReference Include="System.Text.Json" Version="4.7.2" />
  </ItemGroup>

</Project>
